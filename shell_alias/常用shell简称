# 在.bashrc中加上以下常用命令

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ......="cd ../../../../.."


alias C="clear"

alias l="ls -la"
alias ll="ls -l"

alias gs="git status"
alias gp="git pull"

alias sc="vi ~/.bashrc"
alias scs=". ~/.bashrc"

alias dps="docker ps"
alias dpsa="docker ps -a"
alias di="docker images"
alias dl="docker logs"
alias drm="docker rm"
alias drmi="docker rmi"
